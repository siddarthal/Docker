FROM node:16-alpine
WORKDIR /usr/src/app
COPY /material-applications/example-frontend/package*.json .
RUN npm  install
COPY /material-applications/example-frontend .
ENV REACT_APP_BACKEND_URL=http://:8080
RUN npm run build
RUN npm install -g serve
EXPOSE 5000
ENTRYPOINT ["serve","-s","-l","5000","build"]


